<routes>
    <vTypeDistribution id="mixed">
        <vType id="car" vClass="passenger" desiredMaxSpeed="13.88" latAlignment="center" probability="0.9" minGapLat="0.5"/>
	    <vType id="bike" vClass="bicycle" desiredMaxSpeed="5.55" latAlignment="right" probability="0.1" minGapLat="1.5"/>
    </vTypeDistribution>


    <route id="r0" edges="beg1 end1"/>
